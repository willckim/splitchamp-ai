{
  "expo": {
    "name": "SplitChamp AI",
    "slug": "splitchamp-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "splitchamp",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.yourorg.splitchamp",
      "buildNumber": "1",
      "supportsTablet": true
    },
    "android": {
      "package": "com.yourorg.splitchamp",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-mail-composer",
      "expo-web-browser",
      "expo-asset",
      [
        "sentry-expo",
        {
          "enableInExpoDevelopment": false
        }
      ]
    ],
    "extra": {
      "apiBase": "https://your-backend.example.com",
      "supportEmail": "williamckim11@gmail.com",
      "privacyUrl": "https://yourusername.github.io/splitchamp-ai/privacy.html",
      "termsUrl": "https://yourusername.github.io/splitchamp-ai/terms.html",
      "eas": {
        "projectId": "REPLACE_WITH_EAS_PROJECT_ID"
      }
    }
  }
}
